{
    "name": "mochiwa/apache",
    "description": "*",
    "authors": [
        {
            "name": "chiappelloni",
            "email": "chiappelloni.nicolas@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "Framework\\" : "./src/framework",
            "Framework\\Router\\" : "./src/framework/router",
            "Framework\\Renderer\\" : "./src/framework/renderer",
            "Framework\\DependencyInjection\\" : "./src/framework/dependencyInjection",
            "Framework\\Module\\" : "./src/framework/module",
            "Framework\\Middleware\\" : "./src/framework/middleware",
            
            "App\\" : "./src/app",
            "App\\Controller\\" : "./src/app/controller",
            "App\\User\\": "./src/app/user",
            "App\\User\\Controller\\": "./src/app/user/controller",
            "App\\User\\View\\": "./src/app/user/view",
            
            "App\\Error\\": "./src/app/errorModule"
        }
    },
    
    "require": {
        "psr/http-message" : "^1.0",
        "psr/http-server-middleware": "^1.0",
        "guzzlehttp/psr7": "^1.6",
        "altorouter/altorouter": "^2.0.1",
        "http-interop/response-sender": "^1.0",
        "psr/container": "^1.0"
    },
    
    
    "autoload-dev":{
        "psr-4":{
            "Test\\Framework\\Router\\" : "./test/framework/router/",
            "Framework\\Renderer\\" : "./test/framework/renderer",
            
            
            "Test\\App\\": "./test/app/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.5",
        "middlewares/whoops": "^1.2"
        
    }
}
