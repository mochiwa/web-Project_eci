{
    "name": "mochiwa/apache",
    "description": "*",
    "authors": [
        {
            "name": "chiappelloni",
            "email": "chiappelloni.nicolas@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "Framework\\" : "./src/framework",
            "Framework\\Router\\" : "./src/framework/router",
            "Framework\\DependencyInjection\\" : "./src/framework/dependencyInjection",
            "Framework\\Renderer\\" : "./src/framework/renderer",
            "Framework\\HtmlBuilder\\" : "./src/framework/htmlBuilder",
            "Framework\\Module\\" : "./src/framework/module",
            "Framework\\Middleware\\" : "./src/framework/middleware",
            "Framework\\Validator\\" : "./src/framework/validator",
            "Framework\\Session\\" : "./src/framework/session",
            
            "App\\" : "./src/app",
            "App\\htmlBuilder\\" : "./src/app/htmlBuilder/",
            
            
            "App\\User\\": "./src/app/user",
            "App\\User\\Controller\\": "./src/app/user/controller",
            "App\\User\\View\\": "./src/app/user/view",
            
            
            "App\\Article\\" : "./src/app/articleModule",
            "App\\Article\\Controller\\" : "./src/app/articleModule/controller",
            "App\\Article\\Model\\" : "./src/app/articleModule/model",
            
            "App\\Article\\Model\\Article\\" : "./src/app/articleModule/model/article",
            "App\\Article\\Model\\Article\\Service\\" : "./src/app/articleModule/model/article/service",
            "App\\Article\\Model\\Article\\Service\\Request\\" : "./src/app/articleModule/model/article/service/request",
            "App\\Article\\Model\\Article\\Service\\Response\\" : "./src/app/articleModule/model/article/service/response",
            
            "App\\Article\\View\\" : "./src/app/articleModule/view",
            "App\\Article\\Validation\\" : "./src/app/articleModule/validation",
            "App\\Article\\Infrastructure\\Persistance\\InMemory\\" : "./src/app/articleModule/infrastructure/persistance/inMemory"
            
            
        }
    },
    
    "require": {
        "psr/http-message" : "^1.0",
        "psr/http-server-middleware": "^1.0",
        "guzzlehttp/psr7": "^1.6",
        "altorouter/altorouter": "^2.0.1",
        "http-interop/response-sender": "^1.0",
        "psr/container": "^1.0",
        "vlucas/valitron": "^1.4"
    },
    
    
    "autoload-dev":{
        "psr-4":{
            "Test\\Framework\\Router\\" : "./test/framework/router",
            "Test\\Framework\\DependencyInjection\\" : "./test/framework/dependencyInjection",
            "Test\\Framework\\Middleware\\" : "./test/framework/middleware",
            "Test\\Framework\\Renderer\\" : "./test/framework/renderer",
            "Test\\Framework\\HtmlBuilder\\" : "./test/framework/htmlBuilder",
            
            
            "Test\\App\\": "./test/app/",
            "Test\\App\\Article\\Model\\Article\\" : "./test/app/articleModule/model/article",
            "Test\\App\\Article\\Infrastructure\\Persistance\\" : "./test/app/articleModule/infrastructure/persistance"

        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.5",
        "middlewares/whoops": "^1.2"
        
    }
}
