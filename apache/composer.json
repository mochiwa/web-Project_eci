{
    "name": "mochiwa/apache",
    "description": "*",
    "authors": [
        {
            "name": "chiappelloni",
            "email": "chiappelloni.nicolas@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "Framework\\" : "./src/framework",
            "Framework\\Router\\" : "./src/framework/router",
            
            "App\\" : "./src/app",
            "App\\Controller\\" : "./src/app/controller"
        }
    },
    
    "require": {
        "psr/http-message" : "^1.0",
        "psr/http-server-middleware": "^1.0",
        "guzzlehttp/psr7": "^1.6",
        "altorouter/altorouter": "^2.0.1"
    },
    
    
    "autoload-dev":{
        "psr-4":{
            "Test\\Framework\\Router\\" : "./test/framework/router/",
            "Test\\App\\": "./test/app/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.5"
    }
}
